    <script>
        function recaptchaCallback_contact() {
            var btnContact = document.getElementById("btnContact");
            if (btnContact.classList.contains("hidden")) {
                btnContact.classList.remove("hidden");
            }
        }

        function recaptchaCallback_newsletter() {
            var btnConfirmNewsletter = document.getElementById("btnConfirmNewsletter");
            if (btnConfirmNewsletter.classList.contains("hidden")) {
                btnConfirmNewsletter.classList.remove("hidden");
            }
        }
    </script>


