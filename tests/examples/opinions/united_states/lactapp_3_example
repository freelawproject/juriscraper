        //Display search
        function ShowModal(modal) {
            $(modal).modal();
        }

        //Close Modal
        function CloseModal(modal) {
            $(modal).modal();
            $(modal).modal('hide')
        }

        //Disable Enter Key on Select Input Controls
        function disable_enter(e) {
            var keyCode = e.keyCode || e.which;
            if (keyCode === 13) {
                e.preventDefault();
                return false;
            }
        }
    </script>
