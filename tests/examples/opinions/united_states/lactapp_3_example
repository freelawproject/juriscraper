    $create(Sys.Extended.UI.TextBoxWatermarkBehavior, {"ClientStateFieldID":"TextBoxWatermarkExtender12_ClientState","id":"TextBoxWatermarkExtender12","watermarkText":"Email"}, null, null, $get("txtContactEmail"));
});

document.getElementById('RequiredFieldValidator20').dispose = function() {
    Array.remove(Page_Validators, document.getElementById('RequiredFieldValidator20'));
}

